<Window x:Class="apnea_demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:local="clr-namespace:apnea_demo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <oxy:PlotView Name="PlotView" Model="{Binding PlotModel}" />
        <Label x:Name="label" Content="{Binding DeviceInfo}" Margin="10,0,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
